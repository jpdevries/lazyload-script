{
  "name": "lazyload-script",
  "version": "1.0.6",
  "description": "Promise based method for adding a script to the page if it has not already been added",
  "main": "dist/lazyload-script.js",
  "scripts": {
    "test": "./node_modules/.bin/karma start karma.config.js --single-run --browsers PhantomJS",
    "testserve": "karma start karma.config.js",
    "testphantom": "karma start karma.config.js --browsers PhantomJS",
    "gzipped": "gzipped dist/lazyload-script.*.js && gzipped dist/lazyload-script.*.min.js",
    "preinstall": "npx ./bin/preinstall.js",
    "install": "babel lazyload-script.js --out-file dist/lazyload-script.js --presets @babel/preset-env"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jpdevries/lazyload-script.git"
  },
  "keywords": [
    "lazy",
    "load",
    "lazyload",
    "js",
    "promise",
    "component"
  ],
  "author": {
    "name": "JP de Vries",
    "email": "mail@devries.jp",
    "url": "https://github.com/jpdevries"
  },
  "license": "MIT",
  "devDependencies": {
    "gzipped": "^0.0.5",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "phantomjs-polyfill-object-assign": "^0.0.2",
    "promise-polyfill": "^6.0.2",
    "unminified-webpack-plugin": "^1.2.0",

    "@babel/core": "^7.4.3",
    "@babel/cli": "^7.4.3",
    "@babel/preset-env": "^7.4.2"
  },
  "peerDependencies": {
    "webpack": "^4.39.1",
    "webpack-dev-server": "^3.3.1",
    "@babel/core": "^7.4.3",
    "@babel/cli": "^7.4.3",
    "@babel/preset-env": "^7.4.2"
  }
}
